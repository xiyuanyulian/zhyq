{
  "name": "ct-ng",
  "version": "0.0.1",
  "license": "MIT",
  "engines": {
    "node": ">= 8.9.0",
    "npm": ">= 3.0.0"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve --proxy-config proxy.conf.json --host=127.0.0.1",
    "start:hmr": "ng serve --hmr --proxy-config proxy.conf.json --host=0.0.0.0",
    "start:aot": "ng serve --aot --proxy-config proxy.conf.json --host=0.0.0.0",
    "start:prod": "ng serve --configuration production",
    "start:prod:aot": "ng serve --configuration production --aot",
    "build": "node --max_old_space_size=5120 ./node_modules/@angular/cli/bin/ng build --configuration production --base-href /console/ --deploy-url /console/ --build-optimizer",
    "analyze": "npm run build -- --stats-json && npx webpack-bundle-analyzer dist/stats.json",
    "precommit": "npm run lint-staged",
    "lint": "npm run lint:ts && npm run lint:style",
    "lint:ts": "npx ng lint",
    "lint:style": "stylelint src/**/*.less --syntax less",
    "lint-staged": "npx lint-staged",
    "eslint": "npx eslint src/**/*.ts",
    "e2e": "ng e2e",
    "test": "ng test --watch",
    "test-coverage": "ng test --code-coverage --watch=false"
  },
  "dependencies": {
    "@angular/animations": "13.2.2",
    "@angular/cdk": "12.0.4",
    "@angular/common": "13.2.2",
    "@angular/compiler": "13.2.2",
    "@angular/core": "13.2.2",
    "@angular/forms": "13.2.2",
    "@angular/localize": "13.2.2",
    "@angular/platform-browser": "13.2.2",
    "@angular/platform-browser-dynamic": "13.2.2",
    "@angular/router": "13.2.2",
    "@antv/data-set": "~0.10.0",
    "@antv/g2": "~3.5.13",
    "@antv/g2-plugin-slider": "~2.1.0",
    "@apollo/client": "~3.3.20",
    "@date-fns/upgrade": "~1.0.3",
    "@ngx-translate/core": "~12.1.2",
    "@ngx-translate/http-loader": "~4.0.0",
    "ajv": "~6.12.0",
    "angular-tree-component": "~8.5.6",
    "angular-web-storage": "~9.0.0",
    "apollo-angular": "~2.6.0",
    "apollo-upload-client": "~12.1.0",
    "chart.js": "~2.9.4",
    "core-js": "~3.6.4",
    "date-fns": "~2.16.1",
    "file-saver": "~2.0.2",
    "font-awesome": "~4.7.0",
    "format-datetime": "~1.0.2",
    "g2": "~2.3.9",
    "graphql": "~15.5.0",
    "graphql-tag": "~2.11.0",
    "jquery": "~3.5.1",
    "js.cookie": "~0.0.4",
    "lodash": "~4.17.20",
    "moment": "~2.29.1",
    "ng-zorro-antd": "~13.0.1",
    "ng2-charts": "~2.3.0",
    "ngx-countdown": "~3.1.0",
    "ngx-perfect-scrollbar": "~7.0.0",
    "ngx-ueditor": "~2.1.0",
    "rxjs": "~6.6.3",
    "screenfull": "~5.0.2",
    "simple-line-icons": "~2.4.1",
    "subscriptions-transport-ws": "~0.9.18",
    "uuid": "8.3.0",
    "zen-observable": "~0.8.15",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "13.2.3",
    "@angular-devkit/core": "13.2.3",
    "@angular-devkit/schematics": "13.2.3",
    "@angular/cli": "13.2.3",
    "@angular/compiler-cli": "13.2.2",
    "@angular/language-service": "13.2.2",
    "@angularclass/hmr": "~3.0.0",
    "@schematics/schematics": "0.901.0",
    "@types/jasmine": "~3.5.8",
    "@types/jasminewd2": "~2.0.8",
    "@types/jquery": "~3.3.33",
    "@types/jszip": "~3.1.7",
    "@types/mockjs": "~1.0.0",
    "@types/node": "~13.9.0",
    "@types/z-schema": "~3.24.0",
    "@typescript-eslint/eslint-plugin": "~4.8.2",
    "@typescript-eslint/parser": "~4.8.2",
    "codecov": "~3.8.1",
    "codelyzer": "~6.0.1",
    "cz-conventional-changelog": "~3.3.0",
    "eslint": "~7.14.0",
    "jasmine": "~3.6.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~6.0.0",
    "js-base64": "~3.6.0",
    "karma": "~6.3.3",
    "karma-chrome-launcher": "~3.1.0",
    "karma-cli": "~2.0.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.5.0",
    "karma-remap-istanbul": "~0.6.0",
    "karma-sauce-launcher": "~4.3.3",
    "less": "~3.12.2",
    "less-loader": "~7.1.0",
    "lint-staged": "~10.5.2",
    "mockjs": "~1.1.0",
    "npm-run-all": "~4.1.2",
    "nz-tslint-rules": "~0.901.2",
    "protractor": "~7.0.0",
    "rxjs-tslint": "~0.1.8",
    "stylelint": "~13.2.0",
    "stylelint-config-standard": "~20.0.0",
    "ts-node": "~9.0.0",
    "tslint": "~6.1.0",
    "typescript": "~4.5.5",
    "webpack-bundle-analyzer": "~4.1.0",
    "xlsx": "~0.16.9"
  },
  "lint-staged": {
    "*.{cmd,html,json,md,sh,txt,xml,yml}": [
      "git add"
    ],
    "*.ts": [
      "npm run lint:ts",
      "prettier --write",
      "git add"
    ],
    "src/**/*.less": [
      "npm run lint:style",
      "prettier --write",
      "git add"
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ],
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
